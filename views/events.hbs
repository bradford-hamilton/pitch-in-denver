
<div class="row">
  {{#each events}}
       <div class="col s12 m4">

<div class="card">
    <div class="card-image waves-effect waves-block waves-light">
      <img class="activator" src="https://dncache-mauganscorp.netdna-ssl.com/thumbseg/630/630099-bigthumbnail.jpg">
    </div>
    <div class="card-content">
      <span class="card-title activator grey-text text-darken-4">{{this.title}}<i class="material-icons right">more_vert</i></span>
      <p><a href="#">This is a link</a></p>
    </div>
    <div class="card-reveal">
      <span class="card-title grey-text text-darken-4">{{this.title}}<i class="material-icons right">close</i></span>
      <p>{{this.address}}</p>
      <p>{{this.details}}</p>
      <p>Date:{{this.date}}</p>
      <p>Start Time: {{this.start_time}}</p>
      <p>End Time: {{this.end_time}}</p>
    </div>
  </div>
</div>
  {{/each}}
</div>
